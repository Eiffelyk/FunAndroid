<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fixFragment
        android:id="@+id/navigation_home"
        android:name="com.eiffelyk.ft_home.ui.home.HomeFragment"
        android:label="首页"
        tools:layout="@layout/fragment_home" />

    <fixFragment
        android:id="@+id/navigation_project"
        android:name="com.eiffelyk.ft_home.ui.project.ProjectFragment"
        android:label="项目"
        tools:layout="@layout/fragment_project" />

    <fixFragment
        android:id="@+id/navigation_nav"
        android:name="com.eiffelyk.ft_home.ui.navi.NaviFragment"
        android:label="导航"
        tools:layout="@layout/fragment_nav" />
    <fixFragment
        android:id="@+id/navigation_tree"
        android:name="com.eiffelyk.ft_home.ui.tree.TreeFragment"
        android:label="体系"
        tools:layout="@layout/fragment_tree" />
    <fixFragment
        android:id="@+id/navigation_mine"
        android:name="com.eiffelyk.ft_home.ui.mine.MineFragment"
        android:label="我的"
        tools:layout="@layout/fragment_mine" />
</navigation>